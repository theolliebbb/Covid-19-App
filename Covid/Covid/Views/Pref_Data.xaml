<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Covid.Views.Pref_Data" BackgroundColor="Black">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="Center" VerticalOptions="Center">
            <Label x:Name="nameja" VerticalOptions="Center" TextColor="GhostWhite" HorizontalTextAlignment="Center" HorizontalOptions="CenterAndExpand"/>
            <Label x:Name="nameen" VerticalOptions="Center" TextColor="GhostWhite" HorizontalOptions="CenterAndExpand"/>
            <Label x:Name="cases" VerticalOptions="Center" TextColor="GhostWhite" HorizontalOptions="CenterAndExpand"/>
            <Label x:Name="death" VerticalOptions="Center" TextColor="GhostWhite" HorizontalOptions="CenterAndExpand"/>
            <Button x:Name="back" VerticalOptions="Center" HorizontalOptions="CenterAndExpand" Text="Back" Clicked="back_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
